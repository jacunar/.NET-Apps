<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsHttpBinding>
                <binding name="WSHttpBinding_ICalculadoraService">
                    <security mode="Message">
                        <message clientCredentialType="UserName" />
                    </security>
                </binding>
            </wsHttpBinding>
        </bindings>
		<behaviors>
			<endpointBehaviors>
			    <behavior name="ClientCertificateBehavior">
				    <clientCredentials>
					    <serviceCertificate>
						    <authentication certificateValidationMode="PeerOrChainTrust" revocationMode="NoCheck" />
					    </serviceCertificate>
				    </clientCredentials>
			    </behavior>		        
		    </endpointBehaviors>
		</behaviors>
        <client>
            <endpoint address="http://localhost:54542/CalculadoraService.svc"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_ICalculadoraService" behaviorConfiguration="ClientCertificateBehavior"
                contract="CalculadoraService.ICalculadoraService" name="WSHttpBinding_ICalculadoraService">
                <identity>
                    <certificate encodedValue="" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>